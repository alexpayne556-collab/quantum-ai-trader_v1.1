    "InsightFeed": {
      "realTimeUpdates": true,
      "expandableRationale": true,
      "sentimentBasedStyling": true
    },
    "Dashboard": {
      "realTimeUpdates": true,
      "systemHealth": true,
      "marketOverview": true
    }
  },
  "status": "ready"

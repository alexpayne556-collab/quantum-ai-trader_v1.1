# Sick Awesome Modules - Environment Variables
# Copy this file to .env and add your actual API keys

# Data Source API Keys (Required)
POLYGON_API_KEY=your_polygon_api_key_here
FINANCIALMODELINGPREP_API_KEY=your_fmp_api_key_here
TWELVEDATA_API_KEY=your_twelvedata_api_key_here
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# Optional: Additional Data Sources
FINNHUB_API_KEY=your_finnhub_key_here
TIINGO_API_KEY=your_tiingo_key_here
MASSIVE_API_KEY=your_massive_key_here

# System Configuration
DATA_PRIORITY=polygon,fmp,twelvedata,alpha_vantage
LOG_LEVEL=INFO
MAX_REQUESTS_PER_MINUTE=100
CIRCUIT_BREAKER_THRESHOLD=5

# Trading Configuration
DEFAULT_RISK_PER_TRADE=0.02
MAX_POSITION_SIZE=0.25
MIN_CONFIDENCE_THRESHOLD=0.70
INITIAL_CAPITAL=10000

# Database (Optional)
DATABASE_URL=sqlite:///./trading_system.db

# Redis (Optional for caching and real-time)
REDIS_URL=redis://localhost:6379

# WebSocket Configuration
WS_HOST=0.0.0.0
WS_PORT=8090

# Security
SECRET_KEY=your_secret_key_for_jwt
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Performance Settings
ENABLE_UVLOOP=true
ENABLE_ORJSON=true
ASYNC_TIMEOUT=30

# Monitoring (Optional)
SENTRY_DSN=your_sentry_dsn_here
POSTHOG_API_KEY=your_posthog_key_here

# Development
DEBUG=false
TEST_MODE=false
